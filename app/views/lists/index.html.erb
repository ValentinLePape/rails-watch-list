<div class="container w-25">
  <h1>Lists:</h1>
  <ul class="list-group mb-5">
    <% @lists.each do |list| %>
    <li class="list-group-item" aria-current="true"><%= link_to list.name, list_path(list) %></li>
      <% end %>
  </ul>
  <%= link_to "New List", new_list_path, class: "btn btn-outline-primary" %>
</div>
